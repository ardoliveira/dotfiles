! MyNFont=11 sets font size to 11.
!#define MyNFont  xft:Inconsolata:regular:antialias=true:size
!#define MyBFont  xft:Inconsolata:regular:antialias=true:size
!#define MyIFont  xft:Inconsolata:italic:antialias=true:size
!#define MyBIFont xft:Inconsolata:italic:antialias=true:size

!#define MyNFont  xft:Inconsolata for Powerline:medium:antialias=true:size
!#define MyBFont  xft:Inconsolata for Powerline:medium:antialias=true:size
!#define MyIFont  xft:Inconsolata for Powerline:italic:antialias=true:size
!#define MyBIFont xft:Inconsolata for Powerline:italic:antialias=true:size

!#define MyNFont  xft:Noto Mono:medium:antialias=true:size
!#define MyBFont  xft:Noto Mono:medium:antialias=true:size
!#define MyIFont  xft:Noto Mono:italic:antialias=true:size
!#define MyBIFont xft:Noto Mono:italic:antialias=true:size

#define MyNFont  xft:Noto Mono for Powerline:medium:antialias=true:size
#define MyBFont  xft:Noto Mono for Powerline:medium:antialias=true:size
#define MyIFont  xft:Noto Mono for Powerline:italic:antialias=true:size
